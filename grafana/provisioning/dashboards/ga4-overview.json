        "format": "table",
        "rawSql": "WITH current_period AS (SELECT SUM(conversions) as val FROM gsc.fact_ga4_daily WHERE date >= CURRENT_DATE - INTERVAL '${days} days' AND ($property = 'All' OR property = '$property')), previous_period AS (SELECT SUM(conversions) as val FROM gsc.fact_ga4_daily WHERE date >= CURRENT_DATE - INTERVAL '${days} days' * 2 AND date < CURRENT_DATE - INTERVAL '${days} days' AND ($property = 'All' OR property = '$property')) SELECT ROUND(((c.val - p.val)::numeric / NULLIF(p.val, 0) * 100), 1) as \"Change %\" FROM current_period c, previous_period p",
        "refId": "A"
      }],
      "options": {

    {
      "id": 13,
      "title": "Sessions Change",
      "description": "Sessions comparison: current period vs previous period",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 6},
      "targets": [{
        "datasource": {"type": "grafana-postgresql-datasource", "uid": "postgres-gsc"},
        "format": "table",
        "rawSql": "WITH current_period AS (SELECT SUM(sessions) as val FROM gsc.fact_ga4_daily WHERE date >= CURRENT_DATE - INTERVAL '${days} days' AND ($property = 'All' OR property = '$property')), previous_period AS (SELECT SUM(sessions) as val FROM gsc.fact_ga4_daily WHERE date >= CURRENT_DATE - INTERVAL '${days} days' * 2 AND date < CURRENT_DATE - INTERVAL '${days} days' AND ($property = 'All' OR property = '$property')) SELECT ROUND(((c.val - p.val)::numeric / NULLIF(p.val, 0) * 100), 1) as \"Change %\" FROM current_period c, previous_period p",
        "refId": "A"
      }],
      "options": {
        "reduceOptions": {"values": false, "calcs": ["lastNotNull"]},
        "colorMode": "value",
        "graphMode": "none",
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "#F2495C", "value": null},
              {"color": "#FADE2A", "value": -5},
              {"color": "#73BF69", "value": 5}
            ]
          }
        }
      }
    },

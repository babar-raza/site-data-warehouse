    {
      "id": 103,
      "title": "Top Content",
      "type": "row",
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 19},
      "collapsed": false
    },
    {
      "id": 7,
      "title": "Top Converting Pages",
      "description": "Pages with highest conversions in the selected period",
      "type": "table",
      "gridPos": {"h": 10, "w": 24, "x": 0, "y": 20},
      "targets": [{
        "datasource": {"type": "grafana-postgresql-datasource", "uid": "postgres-gsc"},
        "format": "table",
        "rawSql": "SELECT RTRIM(property, '/') || page_path as full_url, SUM(sessions) as sessions, SUM(conversions) as conversions, ROUND(AVG(conversion_rate)::numeric, 4) as avg_conversion_rate, ROUND(AVG(engagement_rate)::numeric, 4) as avg_engagement_rate, ROUND(AVG(bounce_rate)::numeric, 4) as avg_bounce_rate FROM gsc.fact_ga4_daily WHERE date >= CURRENT_DATE - INTERVAL '${days} days' AND sessions > 0 AND ($property = 'All' OR property = '$property') GROUP BY property, page_path ORDER BY conversions DESC, sessions DESC LIMIT 50",
        "refId": "A"
      }],
      "options": {
        "showHeader": true,
        "sortBy": [{"displayName": "conversions", "desc": true}],
        "cellHeight": "sm"
      },
      "fieldConfig": {
        "defaults": {
          "custom": {"align": "auto", "displayMode": "auto", "filterable": true}
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "full_url"},
            "properties": [{"id": "custom.width", "value": 400}]
          },
          {
            "matcher": {"id": "byName", "options": "sessions"},
            "properties": [
              {"id": "custom.displayMode", "value": "gradient-gauge"},
              {"id": "custom.width", "value": 120}
            ]
          },
          {
            "matcher": {"id": "byName", "options": "conversions"},
            "properties": [
              {"id": "custom.displayMode", "value": "color-background"},
              {"id": "custom.width", "value": 100},
              {"id": "thresholds", "value": {"mode": "absolute", "steps": [{"color": "#3274D9", "value": null}, {"color": "#73BF69", "value": 5}]}}
            ]
          },
          {
            "matcher": {"id": "byName", "options": "avg_conversion_rate"},
            "properties": [{"id": "unit", "value": "percentunit"}]
          },
          {
            "matcher": {"id": "byName", "options": "avg_engagement_rate"},
            "properties": [
              {"id": "unit", "value": "percentunit"},
              {"id": "custom.displayMode", "value": "color-background"},
              {"id": "thresholds", "value": {"mode": "absolute", "steps": [{"color": "#F2495C", "value": null}, {"color": "#FADE2A", "value": 0.2}, {"color": "#73BF69", "value": 0.4}]}}
            ]
          },
          {
            "matcher": {"id": "byName", "options": "avg_bounce_rate"},
            "properties": [
              {"id": "unit", "value": "percentunit"},
              {"id": "custom.displayMode", "value": "color-background"},
              {"id": "thresholds", "value": {"mode": "absolute", "steps": [{"color": "#73BF69", "value": null}, {"color": "#FADE2A", "value": 0.5}, {"color": "#F2495C", "value": 0.7}]}}
            ]
          }
        ]
      }
    },
    {
      "id": 104,
      "title": "Distribution",
      "type": "row",
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 30},
      "collapsed": false
    },
    {
      "id": 17,
      "title": "Engagement Distribution",
      "description": "Distribution of pages by engagement rate",
      "type": "piechart",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 31},
      "targets": [{
        "datasource": {"type": "grafana-postgresql-datasource", "uid": "postgres-gsc"},
        "format": "table",
        "rawSql": "SELECT category, COUNT(*) as count FROM (SELECT CASE WHEN AVG(engagement_rate) >= 0.5 THEN 'High (50%+)' WHEN AVG(engagement_rate) >= 0.3 THEN 'Medium (30-50%)' WHEN AVG(engagement_rate) >= 0.1 THEN 'Low (10-30%)' ELSE 'Very Low (<10%)' END as category FROM gsc.fact_ga4_daily WHERE date >= CURRENT_DATE - INTERVAL '${days} days' AND sessions > 0 AND ($property = 'All' OR property = '$property') GROUP BY property, page_path HAVING COUNT(*) > 0) page_engagement GROUP BY category ORDER BY CASE category WHEN 'High (50%+)' THEN 1 WHEN 'Medium (30-50%)' THEN 2 WHEN 'Low (10-30%)' THEN 3 ELSE 4 END",
        "refId": "A"
      }],
      "options": {
        "displayLabels": ["percent", "value"],
        "legend": {"displayMode": "table", "placement": "right", "values": ["value", "percent"]},
        "pieType": "donut",
        "reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false},
        "tooltip": {"mode": "single"}
      },
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "mappings": []
        }
      }
    },
    {
      "id": 18,
      "title": "Conversion Rate Trend",
      "description": "Daily conversion rate over time",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 31},
      "targets": [{
        "datasource": {"type": "grafana-postgresql-datasource", "uid": "postgres-gsc"},
        "format": "time_series",
        "rawSql": "SELECT date as time, ROUND((SUM(conversions)::numeric / NULLIF(SUM(sessions), 0) * 100), 2) as \"Conversion Rate %\" FROM gsc.fact_ga4_daily WHERE date >= CURRENT_DATE - INTERVAL '${days} days' AND ($property = 'All' OR property = '$property') GROUP BY date ORDER BY date",
        "refId": "A"
      }],

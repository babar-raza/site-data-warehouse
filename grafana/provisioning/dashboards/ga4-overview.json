    {
      "id": 1,
      "title": "Total Sessions",
      "description": "Total sessions in the selected period",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 1},
      "targets": [{
        "datasource": {"type": "grafana-postgresql-datasource", "uid": "postgres-gsc"},
        "format": "table",
        "rawSql": "SELECT SUM(sessions)::bigint as \"Total Sessions\" FROM gsc.fact_ga4_daily WHERE date >= CURRENT_DATE - INTERVAL '${days} days' AND ($property = 'All' OR property = '$property')",
        "refId": "A"
      }],
      "options": {
        "reduceOptions": {"values": false, "calcs": ["lastNotNull"]},
        "colorMode": "value",
        "graphMode": "area",
        "orientation": "auto",
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "#F2495C", "value": null},
              {"color": "#FADE2A", "value": 1000},
              {"color": "#73BF69", "value": 5000}
            ]
          },
          "unit": "short"
        }
      }
    },

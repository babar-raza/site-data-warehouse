            ]
          }
        }
      }
    },
    {
      "id": 15,
      "title": "Engagement Change",
      "description": "Engagement rate comparison: current period vs previous period",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 6},
      "targets": [{
        "datasource": {"type": "grafana-postgresql-datasource", "uid": "postgres-gsc"},
        "format": "table",
        "rawSql": "WITH current_period AS (SELECT AVG(engagement_rate) as val FROM gsc.fact_ga4_daily WHERE date >= CURRENT_DATE - INTERVAL '${days} days' AND sessions > 0 AND ($property = 'All' OR property = '$property')), previous_period AS (SELECT AVG(engagement_rate) as val FROM gsc.fact_ga4_daily WHERE date >= CURRENT_DATE - INTERVAL '${days} days' * 2 AND date < CURRENT_DATE - INTERVAL '${days} days' AND sessions > 0 AND ($property = 'All' OR property = '$property')) SELECT ROUND(((c.val - p.val)::numeric / NULLIF(p.val, 0) * 100), 1) as \"Change %\" FROM current_period c, previous_period p",
        "refId": "A"
      }],
      "options": {
        "reduceOptions": {"values": false, "calcs": ["lastNotNull"]},
        "colorMode": "value",
        "graphMode": "none",
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "#F2495C", "value": null},
              {"color": "#FADE2A", "value": -5},
              {"color": "#73BF69", "value": 5}
            ]
          }
        }
      }
    },
    {
      "id": 16,
      "title": "Conversion Rate",
      "description": "Overall conversion rate for the period",
      "type": "gauge",
      "gridPos": {"h": 4, "w": 6, "x": 18, "y": 6},
      "targets": [{
        "datasource": {"type": "grafana-postgresql-datasource", "uid": "postgres-gsc"},
        "format": "table",
        "rawSql": "SELECT ROUND((SUM(conversions)::numeric / NULLIF(SUM(sessions), 0) * 100), 2) as \"Conversion Rate\" FROM gsc.fact_ga4_daily WHERE date >= CURRENT_DATE - INTERVAL '${days} days' AND ($property = 'All' OR property = '$property')",
        "refId": "A"
      }],
      "options": {
        "reduceOptions": {"values": false, "calcs": ["lastNotNull"]},
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 10,
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "#F2495C", "value": null},
              {"color": "#FADE2A", "value": 1},
              {"color": "#73BF69", "value": 3}
            ]
          }
        }
      }
    },
    {
      "id": 102,
      "title": "Trends",
      "type": "row",
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 10},
      "collapsed": false
    },
    {
      "id": 5,
      "title": "Sessions Over Time",
      "description": "Daily sessions and engaged sessions trend",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 11},
      "targets": [{
        "datasource": {"type": "grafana-postgresql-datasource", "uid": "postgres-gsc"},
        "format": "time_series",
        "rawSql": "SELECT date as time, SUM(sessions) as sessions, SUM(engaged_sessions) as engaged_sessions FROM gsc.fact_ga4_daily WHERE date >= CURRENT_DATE - INTERVAL '${days} days' AND ($property = 'All' OR property = '$property') GROUP BY date ORDER BY date",
        "refId": "A"
      }],
      "options": {
        "tooltip": {"mode": "multi", "sort": "desc"},
        "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["sum", "mean"]}
      },
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "spanNulls": true,
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          },
          "unit": "short"
        }
      }
    },
    {
      "id": 6,
      "title": "GSC Clicks vs GA4 Sessions",
      "description": "Correlation between search clicks and site sessions",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 11},
      "targets": [
        {
          "datasource": {"type": "grafana-postgresql-datasource", "uid": "postgres-gsc"},
          "format": "time_series",
          "rawSql": "SELECT date as time, SUM(clicks) as \"GSC Clicks\" FROM gsc.fact_gsc_daily WHERE date >= CURRENT_DATE - INTERVAL '${days} days' GROUP BY date ORDER BY date",
          "refId": "A"
        },
        {
          "datasource": {"type": "grafana-postgresql-datasource", "uid": "postgres-gsc"},
          "format": "time_series",
          "rawSql": "SELECT date as time, SUM(sessions) as \"GA4 Sessions\" FROM gsc.fact_ga4_daily WHERE date >= CURRENT_DATE - INTERVAL '${days} days' AND ($property = 'All' OR property = '$property') GROUP BY date ORDER BY date",
          "refId": "B"
        }
      ],
      "options": {
        "tooltip": {"mode": "multi", "sort": "desc"},
        "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["sum", "mean"]}
      },
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 5,

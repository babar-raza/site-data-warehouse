      "targets": [
        {
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT\n  split_part(regexp_replace(property, '^https?://', ''), '/', 1) as \"Subdomain\",\n  page_path as \"Page\",\n  COALESCE((item->>'title'), 'Unknown') as \"Insight\",\n  ROUND((item->>'score')::numeric, 2) as \"Score (0=Poor)\"\nFROM (\n  SELECT\n    property,\n    page_path,\n    jsonb_array_elements(\n      CASE \n        WHEN opportunities IS NOT NULL AND jsonb_array_length(opportunities) > 0 THEN opportunities\n        WHEN diagnostics IS NOT NULL AND jsonb_array_length(diagnostics) > 0 THEN diagnostics\n        ELSE '[]'::jsonb\n      END\n    ) as item\n  FROM performance.core_web_vitals\n  WHERE (UPPER(${subdomain}) = 'ALL' OR split_part(regexp_replace(property, '^https?://', ''), '/', 1) = ${subdomain})\n    AND strategy = '$device'\n    AND check_date = (SELECT MAX(check_date) FROM performance.core_web_vitals WHERE (UPPER(${subdomain}) = 'ALL' OR split_part(regexp_replace(property, '^https?://', ''), '/', 1) = ${subdomain}) AND strategy = '$device')\n    AND (opportunities IS NOT NULL OR diagnostics IS NOT NULL)\n) sub\nWHERE (item->>'score') IS NOT NULL\nORDER BY (item->>'score')::numeric ASC\nLIMIT 20",
          "refId": "A"
        }
      ],
      "title": "Top Optimization Opportunities",
      "type": "table"

        "sortBy": [{"desc": false, "displayName": "Priority"}]
      },
      "targets": [{
        "datasource": {"type": "postgres", "uid": "postgres-gsc"},
        "rawSql": "SELECT INITCAP(a.priority) as \"Priority\", a.title as \"Title\", CASE WHEN i.entity_id IS NOT NULL THEN CONCAT(RTRIM(a.property, '/'), i.entity_id) ELSE '-' END as \"Page URL\", LEFT(a.description, 180) || CASE WHEN LENGTH(a.description) > 180 THEN '...' ELSE '' END as \"Description\", REPLACE(INITCAP(REPLACE(a.action_type, '_', ' ')), '_', ' ') as \"Type\", TO_CHAR(a.created_at, 'YYYY-MM-DD') as \"Created\", (CURRENT_DATE - DATE(a.created_at)) as \"Age\" FROM gsc.actions a LEFT JOIN gsc.insights i ON a.insight_id = i.id WHERE a.property = '$property' AND a.status = 'pending' ORDER BY CASE a.priority WHEN 'critical' THEN 1 WHEN 'high' THEN 2 WHEN 'medium' THEN 3 WHEN 'low' THEN 4 ELSE 5 END, a.created_at DESC LIMIT 100",
